{
	"manifest_version": 2,

	"name": "IPCheck",
	"description": "Display Public and External IP",
	"version": "1.6",
  
	"icons": { "16": "icon-16.png",
			   "48": "icon-48.png",
			  "128": "icon-128.png" },
			  
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "main.html",
		"default_title": "IPCheck"
	},
	"permissions": [
		"https://showextip.azurewebsites.net/"
	]
}